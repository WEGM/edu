	<script type="text/javascript">
		var actType = 'share',
			actID = '20161017',
			actFlag = actTimeFlag('2016-10-14 00:00:00', '2016-10-16 00:00:00');
		var pageID = getQueryString('kfrom') + '_' + actType + '-' + actID + '#' + openid;
		var shareData = {
			'imgUrl': 'http://cdn.holdfun.cn/zhima/images/2016812/e02fb07fa03e4470a4c375cb43b6c088.png',
			'link': shareUpdateUrl(location.href),
			'title': '“趣”乐宁英语配音开始了~',
			'desc': '点我报名，乐宁英语期待您的加入。'
		};
