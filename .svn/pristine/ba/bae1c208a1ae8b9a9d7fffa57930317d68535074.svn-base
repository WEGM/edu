		        type: "",
		        showCancelButton: true,
		        confirmButtonColor: "#DD6B55",
		        confirmButtonText: "免费试听",
		        cancelButtonText: "了解乐宁",
		        closeOnConfirm: false,
		        closeOnCancel: true
		    }, function(isConfirm) {
		        if (isConfirm) toUrl('./reserve.html');
		    });
		    setTimeout(function(){$('.menu-nav').trigger('click');}, 1.5e3);
		} else {
	    	H.index.init();
		}
