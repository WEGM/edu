.icon-voice {
	display: inline-block;
	width: 40px;
	height: 40px;
	margin: 0 0 0 6px;
	background: url(../../images/icon-voice.png) no-repeat;
	background-position: -80px 0;
	-webkit-transform: scale(.6);
}
.right .icon-voice {
	background: url(../../images/icon-voice-d.png) no-repeat;
	background-position: -80px 0;
	-webkit-transform: scale(.6) rotate(180deg);
}
.play .icon-voice {
    -webkit-animation-name: playvoice;
    -webkit-animation-duration: 1.2s;
    -webkit-animation-iteration-count: infinite;
    -webkit-animation-timing-function: linear;
    -webkit-animation-timing-function: steps(1);
    -webkit-backface-visibility:hidden;
}
.voice .avatar-box {
	position: absolute;
	height: 42px;
	width: 42px;
	overflow: hidden;
	border: 1px solid #CCC;
	top: 0;
	left: -56px;
}
.voice.right .avatar-box {
	position: absolute;
	height: 42px;
	width: 42px;
	overflow: hidden;
	border: 1px solid #CCC;
	top: 0;
	left: initial;
	right: -56px;
}
.voice em {
	position: absolute;
	font-style: normal;
	font-size: 16px;
	color: #CCC;
	right: 10px;
	top: 10px;
}
.right em {
	left: 10px;
	right: initial;
	color: #529E2C;
}
@-webkit-keyframes playvoice {
    0%,100% {background-position: 0 0;}
    33% {background-position: -40px 0;}
    66% {background-position: -80px 0;}
}
@keyframes playvoice {
    0%,100% {background-position: 0 0;}
    33% {background-position: -40px 0;}
    66% {background-position: -80px 0;}
}
