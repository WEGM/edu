.words {
	clear: both;
    display: block;
    position: relative;
    padding: 8px 13px;
    max-width: 70%;
    font-size: 15px;
    line-height: 25px;
    border: 1px solid #DEDEDE;
    background-color: #FFF;
    vertical-align: middle;
    border-radius: 3px;
    word-break: break-all;
    top: 4px;
    color: #111;
    text-align: left;
    float: left;
    margin: 0 0 22px 56px;
}
.words.right {
    background-color: #b1e866;
    border: 1px solid #B9DEB2;
    float: right;
    // margin-left: calc(30% - 58px);
    // margin-left: -webkit-calc(30% - 58px);
    margin: 0 56px 22px 0;
}
.words:before, .words:after {
    content: '';
    width: 0;
    height: 0;
    font-size: 0;
    overflow: hidden;
    position: absolute;
    border-width: 10px;
    top: 10px;
    border-radius: 3px;
}
.words:before {
    border-style: dashed solid dashed dashed;
    border-color: transparent #DEDEDE transparent transparent;
    left: -20px;
}
.words:after {
    border-style: dashed solid dashed dashed;
    border-color: transparent #FFF transparent transparent;
    left: -19px;
}
.words.right:before {
    border-style: dashed dashed dashed solid;
    border-color: transparent transparent transparent #B9DEB2;
    left: initial;
    right: -20px;
}
.words.right:after {
    border-style: dashed dashed dashed solid;
    border-color: transparent transparent transparent #B1E866;
    left: initial;
    right: -19px;
}
.words .avatar-box {
	position: absolute;
	height: 42px;
	width: 42px;
	overflow: hidden;
	border: 1px solid #CCC;
	top: 0;
	left: -56px;
}
.words.right .avatar-box {
	position: absolute;
	height: 42px;
	width: 42px;
	overflow: hidden;
	border: 1px solid #CCC;
	top: 0;
	left: initial;
	right: -56px;
}

