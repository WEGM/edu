html,
body {
    position: relative;
    height: 100%;
}

.scroll-box {
    width: 100%;
    height: 100%;
    overflow: hidden;
}
.coupon-bg
{
    width: 100%;
}
.swiper-wrapper {}

.fill-infor {
    position: relative;
    margin-top: -4px;
    /*line-height: 120px;*/
    height: 120px;
    text-align: center;
    background: #D91217;
    color: #fff;
    display: table;
    width: 100%;
}
.fill-infor div
{
    display: table-cell;
    vertical-align: middle;
}
.font14
{
    font-size: 14px;
}
.fill-infor i {
    display: inline-block;
    height: 8px;
    width: 8px;
    margin: 0px 2px;
}

i.one {
    background: #008FD0
}

i.two {
    background: #F19900
}

i.three {
    background: #00903D
}

.coupon-list {
    background: #F0F0F0;
    padding: 5px;
    overflow-y: auto;
}
.icon
{
    vertical-align: top;
    position: relative;
    display: inline-block;

    -webkit-box-sizing: border-box;
       -moz-box-sizing: border-box;
            box-sizing: border-box;
   
    margin:0px 2%;
}
.logo-icon
{
    vertical-align: middle;
    width: 20px;
    height: 20px;
    background: url("../images/tip.png") top center no-repeat;
    background-size: 100% 100%;
    top:-2px;
}
.coupon-list li {
    overflow: hidden;
    height: 110px;
    margin-bottom: 5px;
    background: #fff;
    border: 1px solid #ddd;
    border-radius: 5px;
}

.coupon-small
{
    height: 100%;
    width: 110px;
    float:left;
    position: relative;
}
.coupon-small:after
{
    content: url(../images/split.png);
    width: 10px;
    height:96px;
    position: absolute;
    right:0px;
    top:8px;
    transform: scale(0.7,1);
    -webkit-transform: scale(0.7,1);
    overflow: hidden;

}
.coupon-list img
{
    position: relative;
    float: left;
    box-sizing: border-box;
    width: 50px;
    height: 50px;
    padding: 10px;
    top:50%;
    left:50%;
    margin-left: -25px;
    margin-top: -25px;
}
.coupon-list li>div:nth-child(2) {
    box-sizing: border-box;
    height: 60px;
    padding: 28px 0px 10px 20px;
    margin-left: 110px;
}
.coupon-value
{
    color:#E4575B;
    font-size: 18px;
    margin-bottom: 10px;
}
.time-limit
{
    color:#B8B8B8;
}
section.form-infor {
    position: relative;
    padding:10%;
    text-align: center;
    background: #F0F0F0;
}

.form-infor h1 {
    font-size: 20px;
    text-align: center;
}

.form-infor ul {
    margin: 10% 2% 0px 2%;
    text-align: center;
}

.form-infor li {
    text-align: left;
    margin-bottom: 10%;
}

li input[type=text],
select {
    width: 76%;
    height: 36px;
    font-size: 14px;
    color: #4a4a4a;
    box-shadow:2px 2px 2px #ddd;
    text-indent: 5px;
    border-radius:8px;
    margin-left:5%;

}

input::-webkit-input-placeholder {
    color: #bbb;
}
.check-box
{
    width: 76%;
    display: inline-block;
}
.check-box label
{       
    margin-left: 25%;
}

input[type=radio]
{
    outline: none;
    border: none;
    vertical-align:middle;
    height: 22px;
    width:22px;
    margin-right: 4px;
    background: url("../images/icheck.png") top center;
    background-size:100% 100%;
}
input[type=radio]:checked
{
    height: 22px;
    width:22px;
    background: url("../images/ichecked.png") top center;
    background-size:100% 100%;
}
.form-infor .btn-submit {
    text-align: center;
    font-size: 16px;
    border-radius: 5px;
    width: 100%;
    position: relative;
    height: 50px;
    display: block;
    color:#fff;
    line-height: 50px;
    background: #D91217;
}
.form-infor .coupon-rule
{
    display: inline-block;
    margin:-5% auto 5% auto;
    color: #4a4a4a;
    text-decoration: underline;
}
section.form-success {
    position: relative;
    padding: 80px 20px;
    text-align: center;
}

.form-success h1 {
    font-size: 21px;
    text-align: center;
}

.form-success p {
    margin: 48px 20px;
}

.form-success .qtcode {
    position: relative;
    width: 150px;
    margin: 0 auto;
    text-align: center;
    background: #eef3fa;
    padding: 20px;
    box-sizing: border-box;
}

.datetime-arr {
    left: 30%;
}

.lq-datetimepick {
    width: 90%;
    left: 5%;
}

dl:nth-child(2) {
    height: auto;
    overflow: hidden;
}

.select-datetime dl.datetime-day {
    height: auto;
}

dl:nth-child(3) {
    border-left: 1px solid #d3dcdd;
}

.datetime-select .selectul {
    width: 120px;
}

.selectul ul li {
    font-size: 14px;
}
.popup {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    z-index: 1;
}
.popup:after {
    position: absolute;
    content: '';
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: #f0f0f0;
}
.popup .content {
    position: absolute;
    top: 10%;
    left: 7%;
    z-index: 9;
    color: #555;
    width: 86%;
    height: 80%;
    overflow: hidden;
    background: #FFF;
    border-radius: 5px;
    padding: 5%;
    box-shadow: 0 0 8px rgba(0,0,0,.3);
}
.popup .content h1 {
    width: 100%;
    font-size: 17px;
    border-bottom: 1px solid #a5c8f1;
    padding: 5% 0 3%;
    margin-bottom: 5%;
}
.popup .content p {
    height: 86%;
    overflow: hidden;
    overflow-y: scroll;
}
.popup .content .btn-close {
    position: absolute;
    width: 26px;
    height: 26px;
    color: #FFF;
    right: 6px;
    top: 9px;
}